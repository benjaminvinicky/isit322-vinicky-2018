{
  "name": "lookup-server",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "express": "4.16.3"
  },
  "scripts": {
    "start": "concurrently 'npm run server' 'npm run client' 'npm run qux' 'npm run gist' 'npm run gitUser' 'npm run git-socket' 'npm run markdown'",
    "server": "babel-node server/bin/www",
    "client": "babel-node start-client.js",
    "qux": "babel-node ../micros/quxMicro/bin/www",
    "gist": "babel-node ../micros/gistMicro/bin/www",
    "gitUser": "babel-node ../micros/gitUserMicro/bin/www",
    "git-socket": "babel-node ../micros/git-socketMicro/bin/www",
    "markdown": "babel-node ../micros/markdownMicro/bin/www",
    "lint": "eslint ."
  },
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "concurrently": "^3.5.1",
    "eslint": "4.19.1",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-import": "2.11.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.7.0"
  }
}
